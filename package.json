{
  "name": "scanbe",
  "version": "1.0.0",
  "main": "dist/src/main.js",
  "scripts": {
    "proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./proto --ts_proto_opt=nestJs=true,outputServices=grpc-js,useOptionals=true --proto_path=./proto ./proto/**/*.proto",
    "build": "npm run prebuild && npx tsc -p tsconfig.json",
    "prebuild": "rimraf dist",
    "start": "node dist/src/main",
    "start:prod": "node dist/src/main",
    "vercel-build": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:debug": "ts-node-dev --inspect --respawn --transpile-only src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "vercel-dev": "npm run start:dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/core": "^11.1.5",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@types/big-integer": "^0.0.30",
    "axios": "^1.11.0",
    "bech32": "^2.0.0",
    "big-integer": "^1.6.52",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "google-protobuf": "^4.0.0",
    "ioredis": "^5.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "ts-proto": "^2.7.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^22.10.2",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^9.16.0",
    "grpc-tools": "^1.13.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
